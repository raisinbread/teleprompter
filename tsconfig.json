{
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "noEmit": true,
    "allowSyntheticDefaultImports": true
  }
}
